{
  "name": "qtbasicutils",
  "version-string": "0.1.0",
  "dependencies": [
    "boost-core",
	"boost-multi-index",
	"boost-bimap",
	"cmake-modules"
  ],
  "features": {
    "qt5": {
      "description": "Use Qt5",
      "dependencies": [
        {
          "name": "linuxdeployqt",
          "features": [
            "qt5"
          ],
          "platform": "linux",
          "default-features": false
        },
        "qt5-base",
        "qt5-tools",
        {
          "name": "qxt",
          "features": [
            "qt5"
          ],
          "default-features": false
        }
      ]
    },
    "qt6": {
      "description": "Use Qt6",
      "dependencies": [
        {
          "name": "linuxdeployqt",
          "features": [
            "qt6"
          ],
          "platform": "linux",
          "default-features": false
        },
        "qtbase",
        "qttools",
		    "qtscxml",
        {
          "name": "qxt",
          "features": [
            "qt6"
          ],
          "default-features": false
        }
      ]
    }
  },
  "default-features": [

  ]
}
